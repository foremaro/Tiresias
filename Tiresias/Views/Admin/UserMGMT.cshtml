@model IEnumerable<Tiresias.Models.User>

<p>
    @Html.ActionLink("Create New", "Register", "Account")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>


        <th>
            @Html.DisplayNameFor(model => model.RoleName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrgName)
        </th>
        
        <th>Manage</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>

   
        <td>
            @Html.DisplayFor(modelItem => item.RoleName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OrgName)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditUser", new { id=item.UserId}) |            
            @Html.ActionLink("Delete", "DeleteUser", new { id = item.UserId })
        </td>
    </tr>
}

</table>
